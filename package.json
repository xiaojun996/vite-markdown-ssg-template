{
  "name": "vite-vue-markdown-ssg-starter",
  "version": "0.1.0",
  "author": "zhixiangyao <blueskyzx@icloud.com> <https://github.com/zhixiangyao>",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhixiangyao/vite-vue-markdown-ssg-starter.git"
  },
  "homepage": "https://github.com/zhixiangyao/vite-vue-markdown-ssg-starter",
  "scripts": {
    "dev": "vite --host",
    "build": "vite-ssg build",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint/",
    "lint:stylelint": "stylelint \"./**/*.{vue,postcss,css}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint-staged": "lint-staged",
    "postinstall": "simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": [
      "eslint --cache --cache-location node_modules/.cache/eslint/"
    ],
    "./**/*.{vue,postcss,css}": [
      "stylelint --cache --cache-location node_modules/.cache/stylelint/"
    ]
  },
  "dependencies": {
    "@unhead/vue": "1.11.14",
    "@vueuse/core": "12.0.0",
    "nprogress": "0.2.0",
    "prism-theme-vars": "0.2.5",
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.13.0",
    "@types/node": "22.10.2",
    "@types/nprogress": "0.2.3",
    "@typescript-eslint/eslint-plugin": "8.18.0",
    "@vitejs/plugin-vue": "5.2.1",
    "dotenv": "16.4.7",
    "eslint": "9.17.0",
    "gray-matter": "4.0.3",
    "lint-staged": "15.2.11",
    "markdown-it-prism": "2.3.0",
    "postcss-html": "1.7.0",
    "rollup": "4.28.1",
    "simple-git-hooks": "2.11.1",
    "stylelint": "16.11.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-order": "6.0.4",
    "typescript": "5.7.2",
    "unplugin-vue-markdown": "0.27.1",
    "vite": "6.0.3",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-pages": "0.32.4",
    "vite-plugin-vue-layouts": "0.11.0",
    "vite-plugin-windicss": "1.9.4",
    "vite-ssg": "0.24.2",
    "vue-tsc": "2.1.10",
    "windicss": "3.5.6"
  },
  "license": "MIT",
  "engines": {
    "node": ">=22",
    "pnpm": ">=9"
  }
}
