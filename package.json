{
  "name": "vite-markdown-ssg-template",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vite-ssg build",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "lint:all": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,js,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{ts,js,json,tsx,jsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/"
  },
  "dependencies": {
    "@vueuse/core": "^5.0.3",
    "@vueuse/head": "^0.6.0",
    "nprogress": "^0.2.0",
    "prism-theme-vars": "^0.2.2",
    "vue": "^3.1.4",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@types/node": "^15.14.0",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "@vitejs/plugin-vue": "^1.2.4",
    "@vue/compiler-sfc": "^3.1.4",
    "@vue/server-renderer": "^3.1.4",
    "dotenv": "^10.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.0.8",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.12.1",
    "gray-matter": "^4.0.3",
    "markdown-it-prism": "^2.1.7",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.3.5",
    "vite": "^2.3.8",
    "vite-plugin-components": "^0.11.5",
    "vite-plugin-md": "^0.6.7",
    "vite-plugin-pages": "^0.13.4",
    "vite-plugin-vue-layouts": "^0.3.1",
    "vite-plugin-windicss": "^1.2.0",
    "vite-ssg": "^0.11.4",
    "vue-tsc": "^0.2.0",
    "windicss": "^3.1.4"
  },
  "engines": {
    "node": "^12 || ^14 || ^15 || ^16"
  }
}
