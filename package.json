{
  "name": "vite-vue-markdown-ssg-starter",
  "version": "0.1.0",
  "author": "zhixiangyao <blueskyzx@icloud.com> <https://github.com/zhixiangyao>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhixiangyao/vite-vue-markdown-ssg-starter.git"
  },
  "homepage": "https://github.com/zhixiangyao/vite-vue-markdown-ssg-starter",
  "scripts": {
    "dev": "vite --host",
    "build": "vite-ssg build",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "lint:all": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,js,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{ts,js,json,tsx,jsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{vue,ts,tsx}": [
      "npx eslint --fix"
    ],
    "src/**/*.{js,json,tsx,css,less,scss,vue,html,md}": [
      "npx prettier --write --loglevel warn "
    ],
    "src/**/*.{vue,less,postcss,css,scss}": [
      "npx stylelint --fix --cache --cache-location node_modules/.cache/stylelint/"
    ]
  },
  "dependencies": {
    "@vueuse/core": "^5.3.0",
    "@vueuse/head": "^0.6.0",
    "aplayer": "^1.10.1",
    "nprogress": "^0.2.0",
    "prism-theme-vars": "^0.2.2",
    "vue": "^3.2.6",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@types/node": "^16.7.2",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vue/compiler-sfc": "^3.2.6",
    "@vue/server-renderer": "^3.2.6",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.16.0",
    "gray-matter": "^4.0.3",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "markdown-it-prism": "^2.2.0",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "typescript": "^4.3.5",
    "vite": "^2.5.1",
    "vite-plugin-components": "^0.13.2",
    "vite-plugin-md": "^0.10.0",
    "vite-plugin-pages": "^0.17.5",
    "vite-plugin-vue-layouts": "^0.3.1",
    "vite-plugin-windicss": "^1.3.0",
    "vite-ssg": "^0.14.6",
    "vue-tsc": "^0.2.3",
    "windicss": "^3.1.7"
  },
  "license": "MIT",
  "engines": {
    "node": "^12 || ^14 || ^15 || ^16"
  }
}
