{
  "name": "vite-vue-markdown-ssg-starter",
  "version": "0.1.0",
  "author": "zhixiangyao <blueskyzx@icloud.com> <https://github.com/zhixiangyao>",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhixiangyao/vite-vue-markdown-ssg-starter.git"
  },
  "homepage": "https://github.com/zhixiangyao/vite-vue-markdown-ssg-starter",
  "scripts": {
    "dev": "vite --host",
    "build": "vite-ssg build",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "lint:all": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,js,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{ts,js,json,tsx,jsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{vue,ts,tsx}": [
      "npx eslint --fix"
    ],
    "src/**/*.{js,json,tsx,css,less,scss,vue,html,md}": [
      "npx prettier --write --loglevel warn "
    ],
    "src/**/*.{vue,less,postcss,css,scss}": [
      "npx stylelint --fix --cache --cache-location node_modules/.cache/stylelint/"
    ]
  },
  "dependencies": {
    "@vueuse/core": "^9.1.1",
    "@vueuse/head": "^0.7.9",
    "aplayer": "^1.10.1",
    "nprogress": "^0.2.0",
    "prism-theme-vars": "^0.2.4",
    "vue": "^3.2.37",
    "vue-router": "^4.1.4"
  },
  "devDependencies": {
    "@rollup/pluginutils": "^4.2.1",
    "@types/node": "^18.7.12",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/server-renderer": "^3.2.37",
    "dotenv": "^16.0.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "fp-ts": "^2.12.2",
    "gray-matter": "^4.0.3",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.1",
    "inferred-types": "^0.22.0",
    "lint-staged": "^13.0.3",
    "markdown-it-prism": "^2.2.5",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "rollup": "^2.78.1",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.7.4",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-md": "^0.20.2",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vite-plugin-windicss": "^1.8.7",
    "vite-ssg": "^0.20.2",
    "vue-tsc": "^0.40.1",
    "windicss": "^3.5.6"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.6.0"
  }
}
