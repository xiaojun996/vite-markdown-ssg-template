{
  "name": "vite-vue-markdown-ssg-starter",
  "version": "0.1.0",
  "author": "zhixiangyao <blueskyzx@icloud.com> <https://github.com/zhixiangyao>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhixiangyao/vite-vue-markdown-ssg-starter.git"
  },
  "homepage": "https://github.com/zhixiangyao/vite-vue-markdown-ssg-starter",
  "scripts": {
    "dev": "vite --host",
    "build": "vite-ssg build",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "lint:all": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,js,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{ts,js,json,tsx,jsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{vue,ts,tsx}": [
      "npx eslint --fix"
    ],
    "src/**/*.{js,json,tsx,css,less,scss,vue,html,md}": [
      "npx prettier --write --loglevel warn "
    ],
    "src/**/*.{vue,less,postcss,css,scss}": [
      "npx stylelint --fix --cache --cache-location node_modules/.cache/stylelint/"
    ]
  },
  "dependencies": {
    "@vueuse/core": "^6.9.1",
    "@vueuse/head": "^0.6.0",
    "aplayer": "^1.10.1",
    "nprogress": "^0.2.0",
    "prism-theme-vars": "^0.2.2",
    "vue": "^3.2.22",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/compiler-sfc": "^3.2.22",
    "@vue/server-renderer": "^3.2.22",
    "dotenv": "^10.0.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "gray-matter": "^4.0.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.2",
    "markdown-it-prism": "^2.2.1",
    "postcss-html": "^1.2.0",
    "prettier": "^2.4.1",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.4.4",
    "unplugin-vue-components": "^0.17.2",
    "vite": "^2.6.14",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-md": "^0.11.4",
    "vite-plugin-pages": "^0.18.2",
    "vite-plugin-vue-layouts": "^0.5.0",
    "vite-plugin-windicss": "^1.5.1",
    "vite-ssg": "^0.16.1",
    "vue-tsc": "^0.29.5",
    "windicss": "^3.2.1"
  },
  "license": "MIT",
  "engines": {
    "node": "^12 || ^14 || ^15 || ^16 || ^17"
  }
}
